<%= form_for @recipe do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br>

  <%= f.fields_for :ingredients do |rec| %>
  <%= rec.label :name %>
  <%= rec.text_field :name %><br>

  <%= rec.label :quantity %>
  <%= rec.text_field :quantity %><br>
  <% end %>

  <%= f.submit %>
<% end %>
